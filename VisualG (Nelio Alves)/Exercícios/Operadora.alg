algoritmo "Operadora"
var
quantidade:inteiro
valor:real
inicio
   escreval("Digite a quantidade de minutos:")
   leia(quantidade)

   valor <- 50
         se quantidade > 100 entao
            valor <- 50 + 2 *(quantidade - 100)
                  fimse
                  
   escreval("O valor a se pagar será de: R$", valor:4:2)
                  
                  
fimalgoritmo
